<%- include('partials/header'); -%>

<h1>This is campgrounds page</h1>

<ul>
  <% campgrounds.forEach(function(campground){%>
  <li>
    <div>
      <h4><%= campground.name %></h4>
      <img src="<%= campground.image %>" alt="<%= campground.name %>" />
    </div>
  </li>
  <% }) %>
</ul>

<%- include('partials/footer'); -%>
